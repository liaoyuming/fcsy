/* member.wxss */
.header {
  background: #78B8E9;
  width: 100%;
  height: 928rpx;
}
.header-text {
  background: white;
  color: #817A8E;
  width: 600rpx;
  height: 224rpx;
  padding: 35rpx;
  font-size: 30rpx;
  border-radius: 14rpx;
  margin: 30rpx auto 0 auto;
}

.header-image {
  width: 400rpx;
  height: 505rpx;
  display: block;
  margin: 10rpx auto 0 auto;
}

.icon {
  width: 55rpx;
  height: 55rpx;
  margin-right: 34rpx;
}

.list {
  width: 100%;
  background: white;
  font-size: 30rpx;
}

.list navigator {
  height: 100rpx;
  display: flex;
  margin: 0 10px;
  border-bottom: 1px solid #E4E6EA;
  justify-content: flex-start;
  align-items: center;
}

.list navigator:last-child {
  border-bottom: none;
}